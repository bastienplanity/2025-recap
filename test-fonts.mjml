<mjml lang="fr">
  <mj-head>
    <mj-font
      name="Inter"
      href="https://fonts.googleapis.com/css2?family=Inter:wght@400;500;600;700&display=block"
    />
    <mj-font
      name="Aeonik"
      href="https://d2skjte8udjqxw.cloudfront.net/fonts/Aeonik/aeonik-regular.css"
    />
  </mj-head>

  <mj-body background-color="#FFFFFF">
    <mj-section padding="40px 20px">
      <mj-column>
        <mj-text
          font-family="serif"
          font-size="32px"
          line-height="40px"
          color="#202020"
          padding="20px 0"
        >
          Je suis Serif
        </mj-text>

        <mj-text
          font-family="sans-serif"
          font-size="32px"
          line-height="40px"
          color="#202020"
          padding="20px 0"
        >
          Je suis Sans-Serif
        </mj-text>

        <mj-text
          font-family="Inter, sans-serif"
          font-size="32px"
          line-height="40px"
          color="#202020"
          padding="20px 0"
        >
          Je suis Inter
        </mj-text>

        <mj-text
          font-family="Aeonik, sans-serif"
          font-size="32px"
          line-height="40px"
          color="#202020"
          padding="20px 0"
        >
          Je suis Aeonik
        </mj-text>
      </mj-column>
    </mj-section>
  </mj-body>
</mjml>
